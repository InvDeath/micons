<?php

/**
 * @file
 * Cleanup micons variables
 */

/**
 * Implements hook_uninstall().
 */
function micons_uninstall() {
  variable_del('micons_file_validate_image_resolution');
  variable_del('micons_default_icon');
  variable_del('micons_image_folder');
  variable_del('micons_position');
  variable_del('micons_image_style_default');
}
